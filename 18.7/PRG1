public class Main
{
public static void main(String[] args)
{
    int myInt=9;
    double myDouble=myInt;
    System.out.println(myInt);
    System.out.println(myDouble);
int sum1=100+50;
    int sum2=sum1+250;
    int sum3=sum2+sum2;
System.out.println(sum1);
    System.out.println(sum2);
    System.out.println(sum3);
}
}
